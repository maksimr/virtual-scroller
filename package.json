{
  "name": "virtual-scroller",
  "version": "0.1.0",
  "description": "",
  "main": "lib/virtual-scroller.js",
  "config": {
    "server": {
      "url": "http://localhost:1234"
    }
  },
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url '.'",
    "lint": "eslint lib/**/*.js",
    "test": "jest ./lib",
    "test:e2e": "jest --config=test/e2e/config.js ./test/e2e"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "eslint": "6.8.0",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "jest-image-snapshot": "2.12.0",
    "jest-puppeteer": "4.4.0",
    "parcel-bundler": "1.12.4",
    "puppeteer": "2.1.1"
  },
  "keywords": []
}
